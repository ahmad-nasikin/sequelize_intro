<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Subjects</title>
  </head>
  <body>
    <center><h1>Subjects</h1></center>
    <div>
      <center>
      <table border="1">
        <tr>
          <th>Id</th>
          <th>Subjects Name</th>
          <th>Action</th>
        </tr>
        <% datas.forEach(d => { %>
        <tr>
        <td><%=d.id%></td>
        <td><%=d.subject_name%></td>
        <td><a href="/contacts/edit/<%= d.id %>"><button type="button" name="button">Edit</button></a></td>
        <td><a href="contacts/delete/<%= d.id %>"><button type="button" name="button">Delete</button></a></td>
      </tr>
      <% }) %>
      </table>
    </center>
    </div>

  </body>
</html>
