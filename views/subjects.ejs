<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Subjects</title>
  </head>
  <body>
    <center><h1>Subject</h1></center>
    <div>
      <center>
      <table border="1">
        <tr>
          <th>Id</th>
          <th>Subject Name</th>
          <th>Action</th>
        </tr>
        <% datas.forEach(d => { %>
        <tr>
        <td><%=d.id%></td>
        <td><%=d.subject_name%></td>
        <td><a href="/subjects/update/<%= d.id %>"><button type="button" name="button">Edit</button></a></td>
        <td><a href="subjects/delete/<%= d.id %>"><button type="button" name="button">Delete</button></a></td>
      </tr>
      <% }) %>
      </table>
      <a href="/subjects/addsubject"><button type="button" name="button">Add Subject</button></a>
    </center>
    </div>
  </body>
</html>
