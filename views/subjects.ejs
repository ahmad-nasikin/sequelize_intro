<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Objects</title>
  </head>
  <body>
    <center><h1>Objects</h1></center>
    <div>
      <center>
      <table border="1">
        <tr>
          <th>Id</th>
          <th>Name</th>
          <th>Company</th>
          <th>Number Telp</th>
          <th>Email</th>
          <th>Action</th>
        </tr>
        <% data.forEach(d => { %>
        <tr>
        <td><%=d.id%></td>
        <td><%=d.name%></td>
        <td><%=d.company%></td>
        <td><%=d.number_telp%></td>
        <td><%=d.email%></td>
        <td><a href="/contacts/edit/<%= d.id %>"><button type="button" name="button">Edit</button></a></td>
        <td><a href="contacts/delete/<%= d.id %>"><button type="button" name="button">Delete</button></a></td>
      </tr>
      <% }) %>
      </table>
    </center>
    </div>

  </body>
</html>
